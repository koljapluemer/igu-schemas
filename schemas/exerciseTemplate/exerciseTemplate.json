{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["id", "instruction", "exerciseType", "generator"],
  "properties": {
    "id": {
      "type": "string"
    },
    "instruction": {
      "type": "string"
    },
    "exerciseType": {
      "$ref": "../exerciseType/exerciseType.json"
    },
    "generator": {
      "$ref": "../generator/generator.json"
    },
    "data": {
      "type": "object",
      "additionalProperties": true
    },
    "blockedBy": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    }
  }
}
